   "source": [
    "s = input()\n",
    "\n",
    "words = s.split()\n",
    "count = 0\n",
    "\n",
    "for w in words:\n",
    "    if len(w) >= 2 and w.isalpha() and w[0].isupper() and w[1:].islower():\n",
    "        count += 1\n",
    "\n",
    "print(count)\n"
   ]
  }
